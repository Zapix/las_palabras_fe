{
  "printWidth": 80,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": false,
  "quoteProps": "as-needed",
  "bracketSameLine": false,
  "trailingComma": "none",
  "bracketSpacing": true,
  "objectWrap": "collapse",
  "arrowParens": "always",
  "endOfLine": "lf",
  "singleAttributePerLine": true
}
